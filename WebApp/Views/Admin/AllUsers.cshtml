@model IEnumerable<UserResponse>
@{
    Layout = "_AdminLayout";
}
<table class="table table-hover table-striped table-light" style="margin-top:100px">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Created By</th>
        <th>Created On</th>
        <th>UserRole</th>
        <th>UserStatus</th>
    </tr>
    
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td>@item.Email</td>
            <td>@item.PhoneNo</td> 
            <td>@item.CreatedBy</td>
            <td>@item.CreatedOn</td>
            <td>@item.UserRole</td>
            <td>@item.UserStatus</td>

        </tr>
    }

</table>